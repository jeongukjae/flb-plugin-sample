        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/bazel-bin/external/msgpack/_virtual_includes/msgpack/*",
                "${workspaceFolder}/bazel-bin/external/spdlog/_virtual_includes/spdlog/*"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
